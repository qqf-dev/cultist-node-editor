{
  "name": "cultist-node-editor",
  "displayName": "cultistVue",
  "description": "visible code editor by node for development of the mod about the game <cultist simulator>",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cultist-node-editor.openEditor",
        "title": "打开密教模拟器节点编辑器"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "check:webview": "node scripts/check-webview.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qqf-dev/cultist-node-editor"
  },
  "dependencies": {
    "json5": "^2.2.3"
  }
}
